{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "/",
    "ignore": [
      "firebase.json",

      "node_modules/**",
      ".firebaserc",
      ".eslintrc.json",
      ".prettierrc.json",
      ".gitignore",
      ".travis.yml",
        ".git/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
